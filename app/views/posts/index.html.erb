<h1>Listing posts</h1>

<table>
  <tr>
    <th>User email</th>
    <th>User</th>
    <th>Qanda</th>
    <th>My answer</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Lock</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.user_email %></td>
    <td><%= post.user_id %></td>
    <td><%= post.qanda_id %></td>
    <td><%= post.my_answer %></td>
    <td><%= post.created_at %></td>
    <td><%= post.updated_at %></td>
    <td><%= post.lock %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
